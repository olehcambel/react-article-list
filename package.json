{
  "name": "1news-article",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^3.6.1",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.3",
    "node-uuid": "^1.4.8",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "concurrently \"npm run start-css\" \"npm run api\"",
    "start-css": "npm-run-all -p watch-css start-js",
    "api": "node server/server.js --inspect",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "build": "npm run build-css && react-scripts build",
    "startDepr": "react-scripts start",
    "buildDepr": "react-scripts build",
    "start-js": "react-scripts start",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/api": {
      "target": "http://0.0.0.0:3001"
    }
  }
}
